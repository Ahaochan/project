<?xml version="1.0" encoding="UTF-8"?>
<process-context xmlns="http://www.w3school.com.cn"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.w3school.com.cn process-engine.xsd">
    <process name="process1">
        <node name="node1" class="com.ruyuan.process.engine.demo.StandardProcessorDemo" next="node2" begin="true"/>
        <node name="node2" class="com.ruyuan.process.engine.demo.RollBackProcessorDemo" next="node3"/>
        <node name="node3" class="com.ruyuan.process.engine.demo.DynamicProcessorDemo" next="node4,node5"/>
        <node name="node4" class="com.ruyuan.process.engine.demo.RollbackProcessorThrowExceptionDemo"/>
        <node name="node5" class="com.ruyuan.process.engine.demo.StandardProcessorDemo"/>
    </process>

    <process name="process-async">
        <node name="node1" class="com.ruyuan.process.engine.demo.StandardProcessorDemo" next="node2" begin="true"/>
        <node name="node2" class="com.ruyuan.process.engine.demo.RollBackProcessorDemo" next="node3"/>
        <node name="node3" class="com.ruyuan.process.engine.demo.DynamicProcessorDemo" next="node4,node5"
              invoke-method="async"/>
        <node name="node4" class="com.ruyuan.process.engine.demo.RollbackProcessorThrowExceptionDemo"/>
        <node name="node5" class="com.ruyuan.process.engine.demo.StandardProcessorDemo"/>
    </process>

    <process name="process2">
        <node name="node1" class="com.ruyuan.process.engine.demo.StandardProcessorDemo" next="node2" begin="true"/>
        <node name="node2" class="com.ruyuan.process.engine.demo.RollBackProcessorDemo" next="node3"/>
        <node name="node3" class="com.ruyuan.process.engine.demo.StandardProcessorDemo" next="node4"/>
        <node name="node4" class="com.ruyuan.process.engine.demo.RollbackProcessorThrowExceptionDemo" next="node5"/>
        <node name="node5" class="com.ruyuan.process.engine.demo.StandardProcessorDemo"/>
    </process>
</process-context>